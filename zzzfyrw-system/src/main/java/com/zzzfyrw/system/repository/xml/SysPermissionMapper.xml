<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzzfyrw.system.repository.mapper.SysPermissionMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zzzfyrw.system.repository.entity.SysPermissionEntity">
        <id column="id" property="id" />
        <result column="row_status" property="rowStatus" />
        <result column="created_time" property="createdTime" />
        <result column="updated_time" property="updatedTime" />
        <result column="created_user" property="createdUser" />
        <result column="updated_user" property="updatedUser" />
        <result column="name" property="name" />
        <result column="type" property="type" />
        <result column="sort" property="sort" />
        <result column="component_url" property="componentUrl" />
        <result column="btn_url" property="btnUrl" />
        <result column="has_show" property="hasShow" />
        <result column="icon" property="icon" />
        <result column="permission_code" property="permissionCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, row_status, created_time, updated_time, created_user, updated_user, name, type, sort, component_url, btn_url, has_show, icon, permission_code
    </sql>

</mapper>
